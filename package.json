{
  "name": "@atomico/router",
  "description": "powerful router for Webcomponents, React and Preact",
  "version": "2.1.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/atomicojs/router.git"
  },
  "author": "UpperCod",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "vitest  run",
    "build:types": "tsc",
    "build:compile": "library \"src/**/*\"",
    "build:exports": "exports \"lib/**/*\" \"types/**/*\" --main router",
    "build:pkg": "npm run build:types && npm run build:compile && npm run build:exports",
    "prepublishOnly": "build:pkg"
  },
  "dependencies": {
    "@atomico/hooks": "^3.50.3",
    "@uppercod/exp-route": "^1.4.0",
    "atomico": "^1.75.2"
  },
  "devDependencies": {
    "@atomico/exports": "^1.13.1",
    "@atomico/tsconfig": "^1.1.2",
    "@atomico/vite": "^2.17.8",
    "@formkit/auto-animate": "^0.8.1",
    "happy-dom": "latest",
    "typescript": "^4.7.4",
    "vite": "^5.0.12",
    "vitest": "^1.2.0"
  }
}
